{
  "rules": {
    "no-unused-vars": "error",
    "no-console": "off",
    "prefer-const": "error",
    "no-var": "error",
    "eqeqeq": "error",
    "no-undef": "error",
    "no-duplicate-imports": "error"
  },
  "env": {
    "node": true,
    "es2022": true,
    "jest": true
  },
  "ignorePatterns": [
    "node_modules/",
    "coverage/",
    "dist/",
    "*.min.js"
  ]
}
